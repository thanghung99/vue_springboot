\"user.tenNguoiDung\"\r\n        />\r\n        <diV>\r\n          <span style=\"color: red\">{{ errName }}</span>\r\n        </diV>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"dc\">Địa chỉ</label>\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"dc\"\r\n          placeholder=\" Địa chỉ\"\r\n          v-model=\"user.diaChi\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"sdt\">Số điện thoại</label>\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"sdt\"\r\n          placeholder=\"Số điện thoại\"\r\n          v-model=\"user.sdt\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"ns\">Ngày sinh</label>\r\n        <input\r\n          type=\"Date\"\r\n          class=\"form-control\"\r\n          id=\"ns\"\r\n          v-model=\"user.ngaySinh\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"qq\">Quê quán</label>\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"qq\"\r\n          placeholder=\"Quê quán\"\r\n          v-model=\"user.queQuan\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"us\">User name</label>\r\n        <input\r\n          @keyup=\"checkValidate\"\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"us\"\r\n          placeholder=\"User name\"\r\n          v-model=\"user.username\"\r\n        />\r\n      </div>\r\n      <diV>\r\n        <span style=\"color: red\">{{ errUserName }}</span>\r\n      </diV>\r\n      <div class=\"form-group\">\r\n        <label for=\"pass\">Password</label>\r\n        <input\r\n          @keyup=\"checkValidate\"\r\n          type=\"password\"\r\n          class=\"form-control\"\r\n          id=\"pass\"\r\n          v-model=\"user.password\"\r\n        />\r\n      </div>\r\n      <diV>\r\n        <span style=\"color: red\">{{ errPassWord }}</span>\r\n      </diV>\r\n      <div class=\"form-group\">\r\n        <label for=\"cf\">Confirm password</label>\r\n        <input\r\n          @keyup=\"checkValidate\"\r\n          type=\"password\"\r\n          class=\"form-control\"\r\n          id=\"cf\"\r\n          v-model=\"user.passwordConfirm\"\r\n        />\r\n      </div>\r\n      <diV>\r\n        <span style=\"color: red\">{{ errConfirmPassWord }}</span>\r\n      </diV>\r\n      <div class=\"form-group\">\r\n        <label for=\"anh\">Ảnh</label>\r\n        <input\r\n          type=\"file\"\r\n          class=\"form-control-file\"\r\n          id=\"anh\"\r\n          v-on:change=\"onFileSelected\"\r\n          ref=\"file\"\r\n        />\r\n      </div>\r\n      <button @click=\"onUpload\" type=\"button\" class=\"btn btn-primary\">\r\n        Update profile\r\n      </button>\r\n    </header>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport UserService from '../services/user.service';\r\n\r\nexport default {\r\n  // name: 'ThietBi',\r\n  data() {\r\n    return {\r\n      errName: '',\r\n      errUserName: '',\r\n      errPassWord: '',\r\n      errConfirmPassWord: '',\r\n      user: {\r\n        tenNguoiDung: '',\r\n        diaChi: '',\r\n        sdt: '',\r\n        ngaySinh: '',\r\n        queQuan: '',\r\n        chucVu: '',\r\n        role: '',\r\n        username: '',\r\n        password: '',\r\n        passwordConfirm: '',\r\n      },\r\n      ErrTenTB: '',\r\n      selectedFile: '',\r\n      message: '',\r\n    };\r\n  },\r\n  mounted() {\r\n    UserService.getProfile().then(\r\n      (response) => {\r\n        this.user = response.data;\r\n        this.selectedFile = this.user.anh;\r\n       // this.user.password = this.user.password;\r\n      },\r\n      (error) => {\r\n        this.content =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n  },\r\n  methods: {\r\n    checkValidate() {\r\n      if (this.user.username) this.errUserName = '';\r\n      if (this.user.tenNguoiDung) this.errName = '';\r\n      if (this.user.passwordConfirm == this.user.password)\r\n        this.errConfirmPassWord = '';\r\n    },\r\n    onFileSelected(event) {\r\n      this.selectedFile = event.target.files[0];\r\n    },\r\n    onUpload() {\r\n      if (!this.user.tenNguoiDung) this.errName = 'Name is required!';\r\n      else if (!this.user.username) this.errUserName = 'UserName is required!';\r\n      else if (this.user.password!=null && this.user.passwordConfirm != this.user.password)\r\n        this.errConfirmPassWord = 'Confirm password faild';\r\n      else\r\n        UserService.updateProfile(\r\n          this.user,\r\n          this.selectedFile\r\n        ).then((response) => {\r\n          if (response.status == 204) alert('User name trùng');\r\n          else alert('Sửa thành công');\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views"}]}