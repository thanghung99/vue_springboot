,KAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;AACA,KAFA;AAGA,GA1BA;AA2BA,EAAA,KAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,WAAA,OAAA,CAAA,IAAA,GAAA,KAAA,QAAA,CAAA,CAAA,CAAA;AACA;AAHA,GA3BA;AAgCA,EAAA,OAAA,EAAA;AACA,IAAA,QAAA,EAAA,oBAAA;AACA,UAAA,CAAA,KAAA,OAAA,CAAA,UAAA,EACA,OAAA,KAAA,QAAA,GAAA,mBAAA;AACA,MAAA,cAAA,CAAA,aAAA,CAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,KAAA,OAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,MAAA,IAAA,GAAA,EAAA,KAAA,CAAA,mBAAA,CAAA,CAAA,KACA,KAAA,CAAA,iBAAA,CAAA;AACA,QAAA,QAAA,CAAA,MAAA;AACA,OAJA;AAKA;AATA;AAhCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <header class=\"jumbotron\">\r\n      <div class=\"form-group\">\r\n        <label for=\"tenThietBi\">Tên thiết bị</label>\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"tenThietBi\"\r\n          placeholder=\"Tên thiết bị\"\r\n          v-model=\"thietBi.tenThietBi\"\r\n        />\r\n        <span style=\"color: red\"> {{ ErrTenTB }}</span>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"hangsx\">Hãng sản xuất</label>\r\n        <input\r\n          type=\"text\"\r\n          class=\"form-control\"\r\n          id=\"hangsx\"\r\n          placeholder=\"Hãng sản xuất\"\r\n          v-model=\"thietBi.hangSX\"\r\n        />\r\n      </div>\r\n\r\n      <select v-model=\"thietBi.loai\" style=\"width: 14%;\r\n    height: 33px;\">\r\n        <option :value=\"loai\" v-for=\"(loai, id) in listLoai\" :key=\"id\">\r\n          {{ loai.tenLoai }}\r\n        </option>\r\n      </select>\r\n      <div class=\"form-group row\">\r\n        <div for=\"example-date-input\" class=\"col-12 col-form-label\">\r\n          Trạng thái\r\n        </div>\r\n      </div>\r\n      <div class=\"form-check\">\r\n        <input\r\n          class=\"form-check-input\"\r\n          type=\"radio\"\r\n          name=\"status\"\r\n          id=\"status1\"\r\n          checked\r\n          v-model=\"thietBi.status\"\r\n          value=\"0\"\r\n        />\r\n        <span class=\"form-check-label\" for=\"flexRadioDefault1\"> True </span>\r\n      </div>\r\n      <div class=\"form-check\">\r\n        <input\r\n          class=\"form-check-input\"\r\n          type=\"radio\"\r\n          name=\"status\"\r\n          id=\"status2\"\r\n          v-model=\"thietBi.status\"\r\n          value=\"1\"\r\n        />\r\n        <span class=\"form-check-label\" for=\"flexRadioDefault2\"> False </span>\r\n      </div>\r\n      <button @click=\"updateTB\" type=\"submit\" class=\"btn btn-primary\">\r\n        UPDATE\r\n      </button>\r\n    </header>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ThietBiService from '../services/thietbi.service';\r\n\r\nexport default {\r\n  name: 'ThietBi',\r\n  data() {\r\n    return {\r\n      listLoai: [],\r\n      thietBi: { tenThietBi: '', hangSX: '', loai: {}, status: 1 },\r\n      ErrTenTB: '',\r\n    };\r\n  },\r\n  mounted() {\r\n    ThietBiService.getThietBi(this.$route.params.id).then(\r\n      (response) => {\r\n        this.thietBi = response.data;\r\n      },\r\n      (error) => {\r\n        this.content =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n    ThietBiService.getLoaiThietBi().then((response) => {\r\n      this.listLoai = response.data;\r\n    });\r\n  },\r\n  watch: {\r\n    listLoai() {\r\n      this.thietBi.loai = this.listLoai[0];\r\n    },\r\n  },\r\n  methods: {\r\n    updateTB: function () {\r\n      if (!this.thietBi.tenThietBi)\r\n        return (this.ErrTenTB = 'Enter name device');\r\n      ThietBiService.updateThietBi(this.$route.params.id,this.thietBi).then((response) => {\r\n        if (response.status == 200) alert('Update thành công');\r\n        else alert('Update thất bại');\r\n        location.reload();\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views"}]}