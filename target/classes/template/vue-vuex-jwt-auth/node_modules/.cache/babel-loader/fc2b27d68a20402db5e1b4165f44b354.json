AA,CAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,MAAA,KAAA,CAAA,UAAA,GAAA,QAAA,CAAA,IAAA;AACA,KAHA;AAIA,GAZA;AAcA,EAAA,OAAA,EAAA;AACA,IAAA,KADA,iBACA,MADA,EACA;AACA,MAAA,aAAA,CAAA,KAAA,CAAA,MAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,MAAA,IAAA,GAAA,EACA,KAAA,CAAA,gBAAA,CAAA;AACA,QAAA,QAAA,CAAA,MAAA;AACA,OAJA;AAKA,KAPA;AAQA,IAAA,KARA,iBAQA,MARA,EAQA;AACA,MAAA,aAAA,CAAA,KAAA,CAAA,MAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,MAAA,IAAA,GAAA,EACA,KAAA,CAAA,gBAAA,CAAA;AACA,QAAA,QAAA,CAAA,MAAA;AACA,OAJA;AAKA;AAdA;AAdA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <header class=\"jumbotron\">\r\n      <!-- <div class=\"input-group\" style=\"width: 28%; margin-bottom: 40px\">\r\n        <input\r\n          type=\"search\"\r\n          class=\"form-control rounded\"\r\n          placeholder=\"Search\"\r\n          aria-label=\"Search\"\r\n          aria-describedby=\"search-addon\"\r\n        />\r\n        <button type=\"button\" class=\"btn btn-outline-primary\">search</button>\r\n      </div> -->\r\n\r\n      <table class=\"table table-responsive  table-striped\">\r\n        <thead>\r\n          <tr>\r\n            <th scope=\"col\">Tên thiết bị</th>\r\n            <th scope=\"col\">Hãng</th>\r\n            <th scope=\"col\">Loại</th>\r\n            <th scope=\"col\">Số lượng</th>\r\n            <th scope=\"col\">Số lượng còn</th>\r\n            <th scope=\"col\">Người mượn</th>\r\n            <th scope=\"col\">Ngày mượn</th>\r\n            <th scope=\"col\">Ngày trả</th>\r\n            <th scope=\"col\">Lý do</th>\r\n            <th scope=\"col\">Thao tác</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"(yeucau, id) in listYeuCau\" :key=\"id\">\r\n            <th scope=\"row\">{{ yeucau.thietBi.tenThietBi }}</th>\r\n            <td>{{ yeucau.thietBi.hangSX }}</td>\r\n            <td>{{ yeucau.thietBi.loai.tenLoai }}</td>\r\n            <td>{{ yeucau.thietBi.loai.soLuong }}</td>\r\n            <td>{{ yeucau.thietBi.loai.soLuongCon }}</td>\r\n            <td>{{ yeucau.taiKhoan.tenNguoiDung }}</td>\r\n            <td>{{ yeucau.ngayMuon | date }}</td>\r\n            <td>{{ yeucau.ngayTra | date }}</td>\r\n            <td>{{ yeucau.lyDo }}</td>\r\n            <td>\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn btn-primary\"\r\n                @click=\"dongY(yeucau)\"\r\n              >\r\n                Đồng ý\r\n              </button>\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn btn-danger\"\r\n                @click=\"huyBo(yeucau)\"\r\n              >\r\n                Hủy bỏ\r\n              </button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </header>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport YeuCauService from '../services/yeucau.service';\r\nexport default {\r\n  name: 'ThietBi',\r\n  data() {\r\n    return {\r\n      listYeuCau: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    YeuCauService.listYeuCau().then((response) => {\r\n      window.console.log(response.data);\r\n      this.listYeuCau = response.data;\r\n    });\r\n  },\r\n\r\n  methods: {\r\n    dongY(yeucau) {\r\n      YeuCauService.dongY(yeucau).then((response) => {\r\n        if(response.status == 200)\r\n            alert(\"Đồng ý yêu cầu\")\r\n            location.reload();\r\n      });\r\n    },\r\n    huyBo(yeucau) {\r\n      YeuCauService.huyBo(yeucau).then((response) => {\r\n        if(response.status == 200)\r\n            alert(\"Hủy bỏ yêu cầu\")\r\n            location.reload();\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views"}]}