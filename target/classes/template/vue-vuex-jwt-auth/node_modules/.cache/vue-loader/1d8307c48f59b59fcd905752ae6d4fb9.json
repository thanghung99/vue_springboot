tr>\r\n            <th scope=\"col\">Tên người dùng</th>\r\n            <th scope=\"col\">Địa chỉ</th>\r\n            <th scope=\"col\">Số điện thoại</th>\r\n            <th scope=\"col\">Ngày sinh</th>\r\n            <th scope=\"col\">Quê quán</th>\r\n            <th scope=\"col\">Chức vụ</th>\r\n            <th scope=\"col\">Role</th>\r\n            <th scope=\"col\">Ảnh</th>\r\n            <th scope=\"col\">Update</th>\r\n            <th scope=\"col\">Delete</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"(user, id) in listUser\" :key=\"id\">\r\n            <th scope=\"row\">{{ user.tenNguoiDung }}</th>\r\n            <td>{{ user.diaChi }}</td>\r\n            <td>{{ user.sdt }}</td>\r\n            <td>{{ user.ngaySinh }}</td>\r\n            <td>{{ user.queQuan }}</td>\r\n            <td>{{ user.chucVu }}</td>\r\n            <td>{{ user.role }}</td>\r\n            <td>\r\n              <img :src=\"user.anh ? require(`@/assets/${user.anh}`) : null\" style=\"max-width: 41px\" />\r\n            </td>\r\n            <td>\r\n              <button type=\"button\" class=\"btn btn-primary\">\r\n                <router-link\r\n                  style=\"color: white\"\r\n                  :to=\"'/user/update/' + user.id\"\r\n                  >Update</router-link\r\n                >\r\n              </button>\r\n            </td>\r\n            <td>\r\n              <button\r\n                type=\"submit\"\r\n                @click=\"deleteUser(user.id)\"\r\n                class=\"btn btn-danger\"\r\n              >\r\n                Delete\r\n              </button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </header>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport UserService from '../services/user.service';\r\n\r\nexport default {\r\n  name: 'ThietBi',\r\n  data() {\r\n    return {\r\n      listUser: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    UserService.getListUser().then(\r\n      (response) => {\r\n        this.listUser = response.data;\r\n      },\r\n      (error) => {\r\n        this.content =\r\n          (error.response &&\r\n            error.response.data &&\r\n            error.response.data.message) ||\r\n          error.message ||\r\n          error.toString();\r\n      }\r\n    );\r\n  },\r\n\r\n  methods: {  \r\n    getImgUrl(anh) {\r\n      return require('../assets/' + anh);\r\n    },\r\n    deleteUser: function (id) {\r\n      UserService.deleteUser(id).then((response) => {\r\n        if (response.status == 200) alert('Xóa thành công');\r\n        else alert('Xóa thất bại');\r\n        location.reload();\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}