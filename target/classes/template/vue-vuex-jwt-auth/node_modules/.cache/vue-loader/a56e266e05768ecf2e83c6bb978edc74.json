AACA;AACA","file":"App.vue","sourceRoot":"src","sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <font-awesome-icon icon=\"user-secret\" />\r\n    <nav class=\"navbar navbar-expand navbar-dark bg-dark\">\r\n      <a href class=\"navbar-brand\" @click.prevent>Kiaisoft</a>\r\n      <div class=\"navbar-nav mr-auto\">\r\n        <li class=\"nav-item\"  v-if=\"currentUser\">\r\n          <router-link to=\"/home\" class=\"nav-link\">\r\n            <font-awesome-icon icon=\"home\" />Home\r\n          </router-link>\r\n        </li>\r\n        <li class=\"nav-item\" v-if=\"currentUser\">\r\n          <router-link v-if=\"!role\" to=\"/thietbi\" class=\"nav-link\"\r\n            >Thiết bị</router-link\r\n          >\r\n        </li>\r\n        <li class=\"nav-item\" v-if=\"currentUser\">\r\n          <router-link v-if=\"!role\" to=\"/user\" class=\"nav-link\"\r\n            >User</router-link\r\n          >\r\n        </li>\r\n        <li class=\"nav-item\" v-if=\"currentUser\">\r\n          <router-link\r\n            v-if=\"role\" \r\n            to=\"/thietbi/yeu-cau-thiet-bi\"\r\n            class=\"nav-link\"\r\n            >Yêu cầu thiết bị</router-link\r\n          >\r\n        </li>\r\n        <li class=\"nav-item\" v-if=\"currentUser\">\r\n          <router-link\r\n            v-if=\"!role\"\r\n            to=\"/thietbi/yeu-cau-xu-ly\"\r\n            class=\"nav-link\"\r\n            >Yêu cầu xử lí</router-link\r\n          >\r\n        </li>\r\n        <li class=\"nav-item\" v-if=\"currentUser\">\r\n          <router-link\r\n            v-if=\"role\"\r\n            to=\"/thietbi/danh-sach-yeu-cau\"\r\n            class=\"nav-link\"\r\n            >Danh sách yêu cầu</router-link\r\n          >\r\n        </li>\r\n        <li class=\"nav-item\" v-if=\"currentUser\">\r\n          <router-link\r\n            v-if=\"role\"\r\n            to=\"/thietbi/danh-sach-thiet-bi\"\r\n            class=\"nav-link\"\r\n            >Danh sách thiết bị</router-link\r\n          >\r\n        </li>\r\n      </div>\r\n      <div v-if=\"!currentUser\" class=\"navbar-nav ml-auto\">\r\n        <!-- <li class=\"nav-item\">\r\n          <router-link to=\"/register\" class=\"nav-link\">\r\n            <font-awesome-icon icon=\"user-plus\" />Sign Up\r\n          </router-link>\r\n        </li> -->\r\n        <li class=\"nav-item\">\r\n          <router-link to=\"/login\" class=\"nav-link\">\r\n            <font-awesome-icon icon=\"sign-in-alt\" />Login\r\n          </router-link>\r\n        </li>\r\n      </div>\r\n\r\n      <div v-if=\"currentUser\" class=\"navbar-nav ml-auto\">\r\n        <li class=\"nav-item\">\r\n          <router-link to=\"/profile\" class=\"nav-link\">\r\n            <font-awesome-icon icon=\"user\" />\r\n            {{ currentUser.username }}\r\n          </router-link>\r\n        </li>\r\n        <li class=\"nav-item\">\r\n          <a class=\"nav-link\" href @click.prevent=\"logOut\">\r\n            <font-awesome-icon icon=\"sign-out-alt\" />LogOut\r\n          </a>\r\n        </li>\r\n      </div>\r\n    </nav>\r\n\r\n\r\n      <router-view @checkUser='checkUser' />\r\n    \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport UserService from './services/user.service';\r\nexport default {\r\n  data() {\r\n    return {\r\n      role: '',\r\n    };\r\n  },\r\n  mounted() {\r\n    UserService.checkRole().then((response) => {\r\n      window.console.log(response.data);\r\n      if (response.data == 'admin') {\r\n        this.role = false;\r\n        window.console.log(this.role)\r\n      }\r\n      if (response.data == 'user') {\r\n        this.role = true;\r\n      }\r\n    });\r\n  },\r\n  computed: {\r\n    currentUser() {\r\n      window.console.log(this.$store.state.auth.user);\r\n      return this.$store.state.auth.user;\r\n    },\r\n    showAdminBoard() {\r\n      if (this.currentUser && this.currentUser.roles) {\r\n        return this.currentUser.roles.includes('ROLE_ADMIN');\r\n      }\r\n\r\n      return false;\r\n    },\r\n    showModeratorBoard() {\r\n      if (this.currentUser && this.currentUser.roles) {\r\n        return this.currentUser.roles.includes('ROLE_MODERATOR');\r\n      }\r\n\r\n      return false;\r\n    },\r\n  },\r\n  methods: {\r\n    logOut() {\r\n      this.role=''\r\n      this.$store.dispatch('auth/logout');\r\n      this.$router.push('/login');\r\n      location.reload()\r\n    },\r\n    checkUser() {\r\n       window.console.log(\"APP\")\r\n       UserService.checkRole().then((response) => {\r\n      window.console.log(response.data);\r\n      if (response.data == 'admin') {\r\n        this.role = false;\r\n        window.console.log(this.role)\r\n      }\r\n      if (response.data == 'user') {\r\n        this.role = true;\r\n      }\r\n    });\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style >\r\n.container{\r\n      min-width: 100%;\r\n}\r\n</style>"]}]}